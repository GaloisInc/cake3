# This Makefile was generated by the Cake3
# https://github.com/grwlf/cake3

GUARD = .GUARD_$(1)_$(shell echo $($(1)) | md5sum | cut -d ' ' -f 1)

ifdef MAIN

# Main section

.PHONY: all
all: ./main.elf
.PHONY: clean
clean: 
	rm ./main.elf ./main\ space.o ./depend.mk
./main.elf: ./main\ space.o
	gcc -o ./main.elf ./main\ space.o
./main\ space.o: ./main\ space.c $(call GUARD,CFLAGS)
	gcc -c $(CFLAGS) -o ./main\ space.o ./main\ space.c
./depend.mk: ./main\ space.c
	gcc -M ./main\ space.c -MF ./depend.mk
$(call GUARD,CFLAGS):
	rm -f .GUARD_CFLAGS_*
	touch $@
include ./depend.mk

else

# Prebuild/postbuild section

.PHONY: all
all: .fix-multy1
.PHONY: clean
clean: .fix-multy1
.PHONY: ./main.elf
./main.elf: .fix-multy1
.PHONY: ./main\ space.o
./main\ space.o: .fix-multy1
.PHONY: ./depend.mk
./depend.mk: .fix-multy1
.INTERMEDIATE: .fix-multy1
.fix-multy1: 
	$(MAKE) MAIN=1 $(MAKECMDGOALS)

endif
