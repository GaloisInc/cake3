# This Makefile was generated by the Cake3
# https://github.com/grwlf/cake3

GUARD = .cake3/GUARD_$(1)_$(shell echo $($(1)) | md5sum | cut -d ' ' -f 1)

ifeq ($(MAIN),1)
unexport MAIN

# Main section

.PHONY: all
all: ./Makefile ./main.elf
./main.elf: ./Makefile ./main\ space.o
	gcc -o ./main.elf ./main\ space.o
./main\ space.o: ./Makefile ./main\ space.c $(call GUARD,CFLAGS)
	gcc -c $(CFLAGS) -o ./main\ space.o ./main\ space.c
./depend.mk: ./Makefile ./main\ space.c
	gcc -M ./main\ space.c -MF ./depend.mk
./Cakegen: ./Cakefile.hs
	cake3
./Makefile: ./Cakegen
	./Cakegen
$(call GUARD,CFLAGS):
	rm -f .cake3/GUARD_CFLAGS_*
	touch $@
include ./depend.mk

else

# Prebuild/postbuild section

ifneq ($(MAKECMDGOALS),clean)

.PHONY: all
all: .fix-multy1
.PHONY: ./main.elf
./main.elf: .fix-multy1
.PHONY: ./main\ space.o
./main\ space.o: .fix-multy1
.PHONY: ./depend.mk
./depend.mk: .fix-multy1
.PHONY: ./Cakegen
./Cakegen: .fix-multy1
.PHONY: ./Makefile
./Makefile: .fix-multy1
.INTERMEDIATE: .fix-multy1
.fix-multy1: 
	-mkdir .cake3
	@echo "*****************"
	@echo "THE CAKE3 EXAMPLE"
	@echo "*****************"
	MAIN=1 $(MAKE) -f ./Makefile $(MAKECMDGOALS)

endif
.PHONY: clean
clean: 
	-rm ./Cakegen ./depend.mk ./main\ space.o ./main.elf
	-rm -rf .cake3

endif
